{
    "name": "@goiam/react",
    "version": "0.2.0",
    "description": "React SDK for Go-IAM - A lightweight Identity and Access Management server",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "rollup -c",
        "build:watch": "rollup -c --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "prepare": "npm run build",
        "dev": "rollup -c --watch",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
        "link:local": "npm run build && npm link",
        "unlink:local": "npm unlink -g @goiam/react",
        "pack:local": "npm pack",
        "install:local": "npm run build && npm pack && npm install -g @goiam/react-*.tgz"
    },
    "keywords": [
        "react",
        "authentication",
        "authorization",
        "iam",
        "identity",
        "access-management",
        "go-iam",
        "sdk"
    ],
    "author": "melvinodsa <melvinodsa@gmail.com>",
    "license": "MIT",
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.0",
        "@babel/preset-env": "^7.22.0",
        "@babel/preset-react": "^7.22.0",
        "@babel/preset-typescript": "^7.22.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^25.0.3",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-typescript": "^11.1.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.3",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.45.0",
        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.6.1",
        "jest-environment-jsdom": "^29.6.1",
        "prettier": "^3.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "^3.26.3",
        "rollup-plugin-dts": "^5.3.0",
        "ts-jest": "^29.4.1",
        "tslib": "^2.8.1",
        "typescript": "^5.1.6"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/melvinodsa/go-iam-sdk.git",
        "directory": "react"
    },
    "bugs": {
        "url": "https://github.com/melvinodsa/go-iam-sdk/issues"
    },
    "homepage": "https://github.com/melvinodsa/go-iam-sdk/tree/main/react#readme"
}